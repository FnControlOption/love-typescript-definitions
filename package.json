{
  "name": "love-typescript-definitions",
  "version": "0.4.0",
  "description": "![Usage GIF](https://media.giphy.com/media/8rEiqcM9BldxRmSMgW/giphy.gif)",
  "scripts": {
    "init": "git submodule update --init",
    "build": "npm run buildsrc && npm run builddef",
    "buildsrc": "npm run clean && moonc -t lua/ src/*.moon && lua ./lua/src/tots.lua > include/love.d.ts",
    "builddef": "lua ./lua/src/tots.lua > include/love.d.ts",
    "test": "tstl -p test",
    "clean": "rm test/test.lua dist/_G.d.ts lua/src/* include/love.d.ts -f",
    "make_global": "cat include/*.d.ts include/love.d.ts >> dist/_G.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hazzard993/love-typescript-definitions.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hazzard993/love-typescript-definitions/issues"
  },
  "homepage": "https://github.com/hazzard993/love-typescript-definitions#readme",
  "devDependencies": {
    "tslint": "^5.12.1",
    "typescript-to-lua": "^0.14.0"
  }
}
